        -- Enum.KeyCode[v] å¯å°‡å­—æ¯è½‰ç‚º Roblox KeyCode
        Window:SetToggleKey(Enum.KeyCode[v])
        print("UI é–‹å•Ÿå¿«æ·éµå·²è¨­å®šç‚º:", v)
    end
})

